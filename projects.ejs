    <ul class="list-group">
      <li class="list-group-header">
        <input class="form-control" type="text" placeholder="Search for a project">
      </li>
      <% for (let i = 0; i < data.projects.length; i++) { %>
        <li class="list-group-item" onclick="setActive(this)">
          <div class="media-body">
            <strong><%= data.projects[i].name %></strong>
            <p><%= data.projects[i].version %>
              <button id="<%= data.projects[i].version %>" path="<%= data.projects[i].path %>" class="btn btn-default btn-project pull-right" onclick="loadUnity(this)">
                <span class="icon icon-play"></span>
              </button>
            </p>
          </div>
        </li>
      <% } %>
    </ul>
  </div>
</div>

